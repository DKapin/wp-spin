{
  "name": "wp-spin",
  "description": "A CLI tool for managing Docker-based WordPress environments",
  "version": "0.1.0",
  "author": "Daniel Kapin",
  "bin": {
    "wp-spin": "./bin/run.js"
  },
  "bugs": "https://github.com/Projects/wp-spinup-v2/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/fs-extra": "^11.0.4",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.5.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/Projects/wp-spinup-v2",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "wp-spin",
    "dirname": "wp-spin",
    "commands": "./dist/commands",
    "commandsDir": "./src/commands",
    "plugins": [],
    "hooks": {},
    "topicSeparator": " ",
    "moduleType": "esm",
    "format": "esm",
    "default": ".",
    "root": ".",
    "ts": true
  },
  "repository": "Projects/wp-spinup-v2",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "deploy": "npm run build && npm run prepack && npm install -g ."
  },
  "types": "./dist/index.d.ts"
}
